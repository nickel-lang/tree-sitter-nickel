{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nickel",
      "camelcase": "Nickel",
      "title": "Nickel",
      "scope": "source.nickel",
      "file-types": [
        "ncl",
        "nickel"
      ],
      "injection-regex": "^(ncl|nickel)$",
      "class-name": "TreeSitterNickel"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nickel grammar for tree-sitter",
    "authors": [
      {
        "name": "Erin van der Veen"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-nickel"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
